{
  "name": "explorook",
  "version": "0.0.7",
  "description": "Rookout's site addon to support local files and folders",
  "main": "dist/index.js",
  "scripts": {
    "publish": "build.sh && build --win -p always",
    "build": "tsc",
    "dist": "build",
    "mac-publish": "export ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true && build --mac --win --linux -p always",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && (set development=1 || export development=1) && electron ./dist/index.js"
  },
  "build": {
    "extends": null,
    "appId": "rookup",
    "publish": [
      {
        "provider": "github",
        "owner": "rookout",
        "repo": "explorook"
      }
    ],
    "directories": {
      "output": "installers"
    },
    "files": [
      "dist",
      "assets",
      "graphql",
      "assets/icons/**/*"
    ],
    "linux": {
      "target": [
        "deb"
      ],
      "category": "Utility"
    },
    "win": {
      "target": "NSIS"
    }
  },
  "repository": "https://github.com/rookout/explorook",
  "keywords": [
    "Rookout",
    "Explorer",
    "ExploRook",
    "Simple-Https"
  ],
  "author": {
    "email": "support@rookout.com",
    "name": "Rookout"
  },
  "license": "MIT",
  "optionalDependencies": {
    "electron-builder-squirrel-windows": "^20.15.0",
    "electron-installer-debian": "^0.8.1"
  },
  "devDependencies": {
    "@playlyfe/gql": "^2.6.0",
    "@types/electron-store": "^1.3.0",
    "electron": "^2.0.2",
    "electron-builder": "^20.15.1",
    "electron-builder-lib": "^20.15.3",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "@material-ui/core": "^1.2.1",
    "@material-ui/icons": "^1.1.0",
    "@types/auto-launch": "^5.0.0",
    "@types/lodash": "^4.14.109",
    "@types/ws": "^5.1.2",
    "auto-launch": "^5.0.5",
    "electron-log": "^2.2.15",
    "electron-store": "^2.0.0",
    "electron-updater": "^2.21.10",
    "graphql-middleware": "^1.3.0",
    "graphql-yoga": "^1.14.8",
    "lodash": "^4.17.10",
    "uuid": "^3.2.1"
  }
}
