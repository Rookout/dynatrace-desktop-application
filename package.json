{
  "name": "rookexp",
  "version": "1.0.0",
  "description": "Rookout's site addon to support local files and folders",
  "build": {
    "appId": "com.rookout.rookup",
    "files": [
      "!.vscode",
      "!src"
    ]
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && (set development=1 || export development=1) && electron ./dist/index.js"
  },
  "repository": "https://github.com/rookout/rookexp",
  "keywords": [
    "Rookout",
    "Explorer",
    "Simple-Https"
  ],
  "author": "Rookout",
  "license": "MIT",
  "devDependencies": {
    "@playlyfe/gql": "^2.6.0",
    "@types/electron-store": "^1.3.0",
    "electron": "^2.0.2",
    "electron-packager": "^12.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.109",
    "@types/ws": "^5.1.2",
    "@material-ui/core": "^1.2.1",
    "@material-ui/icons": "^1.1.0",
    "electron": "^2.0.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "^1.1.4",
    "typeface-roboto": "^0.0.54",
    "uuid": "^3.2.1",
    "electron-store": "^2.0.0",
    "graphql-middleware": "^1.3.0",
    "graphql-yoga": "^1.14.8",
    "lodash": "^4.17.10",
    "vue": "^2.5.16",
    "vue-material": "^1.0.0-beta-10.2"
  }
}